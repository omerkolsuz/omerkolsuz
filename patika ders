import java.util.*;
public class test1{
   public static void main(String[] args){
     Scanner input = new Scanner(System.in);
      int mat, fizik, turkce, tarih, kimya, muzik;
        System.out.print("Matematik kodunuzu giriniz: ");
        mat = input.nextInt();
        System.out.print("Fizik kodunuzu giriniz: ");
        fizik = input.nextInt();
        System.out.print("Turkce kodunuzu giriniz: ");
        turkce = input.nextInt();
        System.out.print("Tarih kodunuzu giriniz: ");
        tarih = input.nextInt();
        System.out.print("Kimya kodunuzu giriniz: ");
        kimya = input.nextInt();
        System.out.print("Muzik kodunuzu giriniz: ");
        muzik = input.nextInt();
        int summa = mat+fizik+turkce+tarih+kimya+muzik;
        int sonuc = summa/5;
      System.out.println("Not ortalamasi :" + sonuc);
   }
}
